****************************************************************************************************************************************
Q2) Reverse an array of integers.

Soln) import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int len = s.nextInt();
        int[] a = new int[len];
        for (int i = 0; i < len ; i++) {
            a[i] = s.nextInt(); 
        }
        for (int i = 0; i < len; i++){
            System.out.print(a[len-i-1] + " ");
        }
    }
}
